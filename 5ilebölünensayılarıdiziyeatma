
package büthazırlık;
public class büthazırlıkdizi3 {
  //1 ile 100 de dahil arasındaki sayılardan 5 ile bölünebilenleri diziye aktarıp diziyi yazdırınız.
    public static void main(String[] args) {
       
        int dizi[]=new int[100];
        int m=0;
        for (int i = 1; i <=100; i++) {
            if(i%5==0){
            dizi[m]=i;
            m++;}
        }
        for (int i = 0; i <m; i++) {
            System.out.println(dizi[i]+" ");
    }
    }   
}
